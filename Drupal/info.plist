<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>action</key>
	<false/>
	<key>category</key>
	<string>SCRIPTS</string>
	<key>command</key>
	<string>#arg handling in alfred sucks, lets split it up
query="{query}"
IFS=" "
set -- $query

if [[ $1 =~ ^[0-9]+$ ]] ; then
  open http://drupal.org/node/{query}
fi</string>
	<key>escapequery</key>
	<false/>
	<key>escapequerybackquotes</key>
	<true/>
	<key>escapequerybrackets</key>
	<true/>
	<key>escapequeryquotes</key>
	<true/>
	<key>escapequerysemicolons</key>
	<true/>
	<key>growloutput</key>
	<false/>
	<key>growloutputsticky</key>
	<false/>
	<key>keyword</key>
	<string>d</string>
	<key>logging</key>
	<true/>
	<key>silent</key>
	<true/>
	<key>subtitle</key>
	<string></string>
	<key>title</key>
	<string>Drupal</string>
</dict>
</plist>
